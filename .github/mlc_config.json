{
    "replacementPatterns": [
        {
            "pattern": "^/(.*)",
            "replacement": "https://nf-co.re/$1"
        }
    ],
    "ignorePatterns": [
        {
            "pattern": "^http://localhost:"
        },
        {
            "pattern": "zoom.us"
        },
        {
            "pattern": "bingo.web.app"
        }
    ],
    "aliveStatusCodes": [200, 301, 403, 429],
    "timeout": "20s",
    "retryOn429": true,
    "retryCount": 5,
    "fallbackRetryDelay": "30s"
}
